    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main Debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Sanbdox Debugger",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "module": "Sandbox.sandbox"
        }
    ]
}